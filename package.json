{
  "name": "graphql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.js",
    "schema": "graphql get-schema -p prisma"
  },
  "husky": {
    "hooks": {
