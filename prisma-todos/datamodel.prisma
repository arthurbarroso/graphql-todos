type Todo {
  id: ID! @id
  title: String!
  content: String!
  done: Boolean!
  author: User! @relation(name: "TodoToUser", onDelete: SET_NULL)
}

type User {
  id: ID! @id
  username: String! @unique
  email: String! @unique
  password: String!
  todos: [Todo!]! @relation(name: "TodoToUser", onDelete: CASCADE)
}